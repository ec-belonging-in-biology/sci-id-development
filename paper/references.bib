@article{lalonde_does_2021,
	title = {Does cultural appropriation cause harm?},
	volume = {9},
	issn = {2156-5503},
	url = {https://doi.org/10.1080/21565503.2019.1674160},
	doi = {10.1080/21565503.2019.1674160},
	abstract = {Cultural appropriation is often called a buzzword and dismissed as a concept for serious engagement. Political theory, in particular, has been largely silent about cultural appropriation. Such silence is strange considering that cultural appropriation is clearly linked to key concepts in political theory such as culture, recognition, and redistribution. In this paper, I utilize political theory to advance a harm-based account of cultural appropriation. I argue that there are three potential harms with cultural appropriation: (1) nonrecognition, (2) misrecognition, and (3) exploitation. Discerning whether these harms are present or absent offers a means of placing specific instances of cultural appropriation on a spectrum of harmfulness. I conclude by considering how cultural appropriation, and associated appropriative harms, may be avoided.},
	number = {2},
